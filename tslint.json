{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-namespace": false,
        "no-console": false,
        "no-trailing-whitespace": false,
        "semicolon": [true, "always", "ignore-bound-class-methods"],
        "no-empty": false,
        "interface-name": false
    },
    "rulesDirectory": []
}